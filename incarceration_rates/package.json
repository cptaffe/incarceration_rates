  "bugs": {
    "url": "https://github.com/cemegginson/incarceration_rates/issues"
  },
  "homepage": "https://github.com/cemegginson/incarceration_rates#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.17",
    "d3": "^3.5.16",
    "es6-shim": "^0.35.0",
    "lodash": "^4.11.2",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "systemjs": "^0.19.26",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
