{
  "name": "incarceration_rates",
  "version": "1.0.0",
  "description": "Information Visualization project to visualize incarceration rates",
  "main": "app.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" \"scss --watch app:app\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cemegginson/incarceration_rates.git"
  },
